@model List<Post>

<div class="text-center">
    <h1 class="display-4">List</h1>

    @foreach (var post in Model)
    {
        <div class="text-left">
            <h3>@post.Title</h3>
            <p>@post.Resume</p>
            <div class="wrapper-disclaimer">
                <span class="disclaimer">@post.Category</span>
            </div>
        </div>
    }
</div>